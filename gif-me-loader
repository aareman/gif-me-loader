import numpy as np
from PIL import Image, ImageSequence

im = Image.open('samples/test1.gif')
lenth = len(list(ImageSequence.Iterator(im)))
height, width = np.array(im).shape
step = width / lenth
i = 0
frames = []
for frame in ImageSequence.Iterator(im):
    image = frame.copy().convert('RGBA')
    temp = np.array(image)
    temp[-5:-1,i*step:i*step + step] = [255, 0, 0, 1]
